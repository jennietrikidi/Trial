
import java.util.Arrays; 
import java.util.Scanner;

public class Costumer extends Person {
Scanner input = new Scanner(System.in);
private int phone;
private String mail;
Costumer[] costumer=new Costumer[0];
public Costumer(String name, String id, int phone, String mail){
    super(name,id);
    this.phone=phone;
    this.mail=mail;
   }
   @Override
public String toString() 
return    super.toString()+"\n"+"phone: " + phone +"\n"+  "mail: " + mail;   
}
}
