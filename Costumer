import java.util.Arrays;
import java.util.Scanner;
public class Costumer extends Person{
    private String id;
    private String phone;
    private String mail;
    private double age;
    public Costumer(String name, String secName,String id, String phone, String mail, double age) {
        super(name, secName);
        this.id = id;
        this.phone = phone;
        this.mail = mail;
        this.age = age;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public double getAge() {
        return age;
    }

    public void setAge(double age) {
        this.age = age;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getMail() {
        return mail;
    }

    public void setMail(String mail) {
        this.mail = mail;
    }

    @Override
    public String toString() {
        return super.toString() +"\n"+
                "id: " + id + "\n" +
                "phone: " + phone + "\n" +
                "mail: " + mail + "\n" +
                "age: " + age;
    }
}
