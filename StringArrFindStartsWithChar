import jdk.jfr.EventFactory;

import java.math.BigDecimal;
import java.util.Arrays;
import java.util.Collections;
import java.util.Random;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        Random random = new Random();
        String[]line={"Dolthin","Kitty","Micro","Helicopter","Almonds","Koala"};
       System.out.println("Enter an uppercase char:");
       char letter =input.next().charAt(0);
       print_words(line,letter);
    }
    public static void print_words(String[]line,char letter){
      for (int i=0;i<line.length;i++){
          if(line[i].charAt(0)==letter){
              System.out.println(line[i]);
          }
      }
    }
}
