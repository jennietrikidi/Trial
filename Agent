import java.util.Arrays;
import java.util.Scanner;

public class  Agent extends Person {
    private int age;
    private int salary;
    public Agent(String name, String workersNum, int age, int salary){
        super(name,workersNum);
        this.age=age;
        this.salary=salary;
    }
    public int getSalary() {
        return salary;
    }

    public void setSalary(int salary) {
        this.salary = salary;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    @Override
    public String toString() {
        return "Agent:" + super.toString()+
                "age: " + age +
                ", salary=" + salary ;
    }
}
