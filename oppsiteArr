public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int[] num = new int[10];
        System.out.println("Enter ten numbers: ");
        for (int i = 0; i < num.length; i++) {
            num[i] = input.nextInt();
        }
        for (int i= num.length-1;i>=0;i--){
            System.out.println(num[i]);
        }
    }
}
